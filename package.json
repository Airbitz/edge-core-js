{
  "name": "airbitz-core-js",
  "version": "0.3.6",
  "description": "Airbitz core Javascript implementation",
  "keywords": [
    "bitcoin",
    "blockchain",
    "ethereum",
    "key management",
    "security"
  ],
  "homepage": "https://airbitz.co",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib/*",
    "package.json"
  ],
  "main": "lib/indexABC.js",
  "module": "lib/indexABC.es.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-core-js.git"
  },
  "scripts": {
    "build": "npm run prepare",
    "build:flow": "flow-copy-source -i '**/*.test.js' src lib",
    "flow": "flow status",
    "format": "prettier --no-semi --single-quote --write '*.js' 'src/**/*.js' && eslint --fix '*.js' 'src/**/*.js'",
    "precommit": "npm run pretest",
    "prepare": "webpack && rollup -c && npm run build:flow",
    "pretest": "webpack && rollup -c rollup.config.test.js",
    "test": "mocha --require source-map-support/register build/tests.cjs.js"
  },
  "dependencies": {
    "aes-js": "^3.1.0",
    "base-x": "^1.0.4",
    "disklet": "^0.1.3",
    "elliptic": "^6.4.0",
    "hash.js": "^1.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rfc4648": "^0.9.1",
    "scrypt-js": "^2.0.3",
    "utf8": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-fast-async": "^6.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.53.1",
    "flow-copy-source": "^1.2.1",
    "husky": "^0.14.3",
    "mocha": "^3.5.0",
    "rollup": "^0.46.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "source-map-support": "^0.4.15",
    "webpack": "^3.5.3"
  }
}
